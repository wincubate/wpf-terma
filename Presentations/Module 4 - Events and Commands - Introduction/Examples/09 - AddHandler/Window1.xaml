<Window x:Class="Wincubate.EventAndCommandsExamples.Slide09.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Routed Events" Height="222" Width="361"
    Button.Click="OnButtonClicked">
    <DockPanel>
        <TextBlock FontSize="24" Margin="20">Bubbling Events</TextBlock>

        <StackPanel Button.Click="OnButtonClicked">
            <Button Margin="20">One</Button>
            <Button Margin="20">Two</Button>
            <Button Margin="20">Three</Button>
        </StackPanel>
    </DockPanel>
</Window>
